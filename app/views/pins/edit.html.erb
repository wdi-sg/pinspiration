
<h1>Edit your pin</h1>

<%= form_with scope: :pin, model: @pin, local: true do |form| %>


  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>

  <p>
    <%= form.label :img_url %><br>
    <%= form.text_field :img_url %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>