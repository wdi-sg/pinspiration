<%= form_with scope: :pin, url: pins_path, local: true do |form| %>
<p></p>
  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>

  <p>
    <%= form.label :media_url %><br>
    <%= form.text_area :media_url %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>


