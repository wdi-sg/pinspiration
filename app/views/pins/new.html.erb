<%= form_with scope: :pin, url: pins_path, local: true do |form| %>
   
    <p>
      <%= form.label :name %>
      <%= form.text_field :name %>
    </p>
  
    <p>
      <%= form.label :description %><br>
      <%= form.text_area :description %>
    </p>
  
    <p>
      <%= form.submit %>
    </p>
  <% end %> 