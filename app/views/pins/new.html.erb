<%= form_with scope: :pin, url: pins_path, local: true do |form| %>

<%= form.collection_check_boxes :tag_ids, @tags, :id, :tagtext %>
  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>

  <p>
    <%= form.label :url %><br>
    <%= form.text_area :url %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>
<%= link_to 'Back', root_path %>