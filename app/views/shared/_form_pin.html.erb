<%= form_with model: @pin, local: true do |form| %>

  <div class="field">
    <%= form.label :title %>
    <%= form.text_area :title %>
  </div>

  <div class="field">
    <%= form.label :image_url %>
    <%= form.text_area :image_url %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
