<p id="notice"><%= notice %></p>

<h1>Tags</h1>

<table class="table table-striped text-center">
  <thead>
    <tr>
      <th>Tag Name</th>
      <th colspan="3">Options</th>
    </tr>
  </thead>

  <tbody>
    <% @tags.each do |tag| %>
      <tr>
        <td><%= tag.tagname %></td>
        <td><%= link_to 'Show', tag %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


